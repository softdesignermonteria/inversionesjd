	<queryString>
		<![CDATA[select concat_ws(' ',nombre1,nombre2,apellido1,apellido2) as nombres,
c.id as id_cliente,c.nit as nit_cli,cr.id,cr.prefijo,cr.consecutivo,fecha_act as fecha_actividad,cr.observaciones,cr.cuotas,cr.valor_cuotas,cr.total_credito,cr.porcentaje,td.nombre as tipo_documento,emp.nombre_empresa,emp.correo,emp.nit as nit_emp,emp.direccion,emp.telefonos,emp.web,
cc.pagado,cc.valor_total,(cc.valor_total+cc.pagado) as saldo
from clientes c,tipo_documento td, creditos cr,cxc cc,empresa emp
where cr.tipo_documento_id=td.id
and cr.clientes_id=c.id
and cr.empresa_id=emp.id
and cc.creditos_id=cr.id
and cr.clientes_id=$P{id_cliente}
and cr.id=$P{id_credito}
group by c.id]]>
	</queryString>
